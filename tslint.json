{
  "extends": [
    "tslint-react",
    "tslint-config-standard",
    "tslint-plugin-cypress",
    "tslint:recommended"
  ],
  "linterOptions": {
    "format": "stylish",
    "exclude": [
      "./dist/**/*"
    ]
  },
  "rules": {
    "semicolon": false,
    "quotemark": false,
    "trailing-comma": false,
    "object-literal-sort-keys": false,
    "space-before-function-paren": false,
    "ordered-imports": false,
    "no-floating-promises": false,
    "curly": false,
    "jsx-boolean-value": false,
    "member-access": false
  },
  "jsRules": {
    "semicolon": false,
    "quotemark": false,
    "trailing-comma": false,
    "object-literal-sort-keys": false,
    "space-before-function-paren": false
  }
}